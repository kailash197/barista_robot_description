<?xml version="1.0" ?>
<robot name="barista_bot">
  <material name="Blue">
    <color rgba="0.0 0.0 1.0 1"/>
  </material>
  <material name="White">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="Black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="Red">
    <color rgba="1 0 0 1"/>
  </material>
  <material name="Green">
    <color rgba="0 1 0 1"/>
  </material>

  <link name="base_link"/>
  <link name="base_plate_link">
    <visual>
      <geometry>
        <cylinder length="0.155" radius="0.178"/>
      </geometry>
      <material name="Blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.155" radius="0.178"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.8"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.007938466666666666" ixy="0" ixz="0" iyy="0.007938466666666666" iyz="0" izz="0.0126736"/>
    </inertial>
  </link>
  <gazebo reference="base_plate_link">
    <material>Gazebo/Blue</material>
  </gazebo>
  <joint name="base_link_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="base_link"/>
    <child link="base_plate_link"/>
  </joint>
  <link name="standoff_rods1_link">
    <visual>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.00040583333333333335" ixy="0" ixz="0" iyy="0.00040583333333333335" iyz="0" izz="5e-06"/>
    </inertial>
  </link>
  <gazebo reference="standoff_rods1_link">
    <material>Gazebo/White</material>
  </gazebo>
  <link name="standoff_rods2_link">
    <visual>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.00040583333333333335" ixy="0" ixz="0" iyy="0.00040583333333333335" iyz="0" izz="5e-06"/>
    </inertial>
  </link>
  <gazebo reference="standoff_rods2_link">
    <material>Gazebo/White</material>
  </gazebo>
  <link name="standoff_rods3_link">
    <visual>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.00040583333333333335" ixy="0" ixz="0" iyy="0.00040583333333333335" iyz="0" izz="5e-06"/>
    </inertial>
  </link>
  <gazebo reference="standoff_rods3_link">
    <material>Gazebo/White</material>
  </gazebo>
  <link name="standoff_rods4_link">
    <visual>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.00040583333333333335" ixy="0" ixz="0" iyy="0.00040583333333333335" iyz="0" izz="5e-06"/>
    </inertial>
  </link>
  <gazebo reference="standoff_rods4_link">
    <material>Gazebo/White</material>
  </gazebo>
  <!-- Standoff Rods Joints-->
  <joint name="standoff_rods1_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.03 0.13 0.1875"/>
    <parent link="base_plate_link"/>
    <child link="standoff_rods1_link"/>
  </joint>
  <joint name="standoff_rods2_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.03 0.13 0.1875"/>
    <parent link="base_plate_link"/>
    <child link="standoff_rods2_link"/>
  </joint>
  <joint name="standoff_rods3_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.03 -0.13 0.1875"/>
    <parent link="base_plate_link"/>
    <child link="standoff_rods3_link"/>
  </joint>
  <joint name="standoff_rods4_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.03 -0.13 0.1875"/>
    <parent link="base_plate_link"/>
    <child link="standoff_rods4_link"/>
  </joint>

  <link name="cup_holder_tray_link">
    <visual>
      <geometry>
        <cylinder length="0.09" radius="0.16"/>
      </geometry>
      <material name="Blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.09" radius="0.16"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.00035375" ixy="0" ixz="0" iyy="0.00035375" iyz="0" izz="0.00064"/>
    </inertial>
  </link>
  <gazebo reference="cup_holder_tray_link">
    <material>Gazebo/Blue</material>
  </gazebo>
  <joint name="cup_holder_tray_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.3425"/>
    <parent link="base_plate_link"/>
    <child link="cup_holder_tray_link"/>
  </joint>
  <!-- Two wheels -->
  <link name="left_wheel_link">
    <visual>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
      <material name="Black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.07"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="2.4158633333333336e-05" ixy="0" ixz="0" iyy="2.4158633333333336e-05" iyz="0" izz="4.3366400000000005e-05"/>
    </inertial>
  </link>
  <gazebo reference="left_wheel_link">
    <material>Gazebo/Black</material>
  </gazebo>
  <link name="right_wheel_link">
    <visual>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
      <material name="Black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.07"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="2.4158633333333336e-05" ixy="0" ixz="0" iyy="2.4158633333333336e-05" iyz="0" izz="4.3366400000000005e-05"/>
    </inertial>
  </link>
  <gazebo reference="right_wheel_link">
    <material>Gazebo/Black</material>
  </gazebo>
  <!-- Wheel Left and Right Joints-->
  <joint name="left_wheel_joint" type="continuous">
    <parent link="base_plate_link"/>
    <child link="left_wheel_link"/>
    <origin rpy="-1.57 0 0" xyz="0.0 0.15 -0.053"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  <joint name="right_wheel_joint" type="continuous">
    <parent link="base_plate_link"/>
    <child link="right_wheel_link"/>
    <origin rpy="-1.57 0 0" xyz="0.0 -0.15 -0.053"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  <gazebo>
    <plugin filename="libgazebo_ros_diff_drive.so" name="barista_bot_differential_drive_controller">
      <!-- wheels -->
      <left_joint>left_wheel_joint</left_joint>
      <right_joint>right_wheel_joint</right_joint>
      <!-- kinematics -->
      <wheel_separation>0.3</wheel_separation>
      <wheel_diameter>0.0704</wheel_diameter>
      <!-- limits -->
      <max_wheel_torque>1.0</max_wheel_torque>
      <max_wheel_acceleration>2.0</max_wheel_acceleration>
      <!-- output -->
      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>
    </plugin>
  </gazebo>
  
  <!-- Caster Wheels -->
  <link name="front_roll_link">
    <visual>
      <origin rpy="1.5707 0 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
      <material name="Red"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.0015"/>
      <origin rpy="1.5707 0 1.5707" xyz="0 0 0"/>
      <inertia ixx="8.75e-08" ixy="0" ixz="0" iyy="8.75e-08" iyz="0" izz="1.6875e-07"/>
    </inertial>
  </link>
  <gazebo reference="front_roll_link">
    <material>Gazebo/Red</material>
  </gazebo>
  <link name="front_pitch_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.015"/>
      </geometry>
      <material name="Green"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.015"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.002"/>
      <inertia ixx="1.8e-07" ixy="0" ixz="0" iyy="1.8e-07" iyz="0" izz="1.8e-07"/>
    </inertial>
  </link>
  <gazebo reference="front_pitch_link">
    <material>Gazebo/Green</material>
  </gazebo>
  <link name="front_yaw_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
      <material name="Blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.0015"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="8.75e-08" ixy="0" ixz="0" iyy="8.75e-08" iyz="0" izz="1.6875e-07"/>
    </inertial>
  </link>
  <gazebo reference="front_yaw_link">
    <material>Gazebo/Blue</material>
  </gazebo>
  <joint name="front_yaw_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0.15 0 -0.073"/>
    <parent link="base_plate_link"/>
    <child link="front_yaw_link"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" velocity="100.0"/>
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <joint name="front_roll_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="front_yaw_link"/>
    <child link="front_roll_link"/>
    <axis xyz="1 0 0"/>
    <limit effort="1000.0" velocity="100.0"/>
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <joint name="front_pitch_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="front_roll_link"/>
    <child link="front_pitch_link"/>
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" velocity="100.0"/>
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <link name="back_roll_link">
    <visual>
      <origin rpy="1.5707 0 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
      <material name="Red"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.0015"/>
      <origin rpy="1.5707 0 1.5707" xyz="0 0 0"/>
      <inertia ixx="8.75e-08" ixy="0" ixz="0" iyy="8.75e-08" iyz="0" izz="1.6875e-07"/>
    </inertial>
  </link>
  <gazebo reference="back_roll_link">
    <material>Gazebo/Red</material>
  </gazebo>
  <link name="back_pitch_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.015"/>
      </geometry>
      <material name="Green"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.015"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.002"/>
      <inertia ixx="1.8e-07" ixy="0" ixz="0" iyy="1.8e-07" iyz="0" izz="1.8e-07"/>
    </inertial>
  </link>
  <gazebo reference="back_pitch_link">
    <material>Gazebo/Green</material>
  </gazebo>
  <link name="back_yaw_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
      <material name="Blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.0015"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="8.75e-08" ixy="0" ixz="0" iyy="8.75e-08" iyz="0" izz="1.6875e-07"/>
    </inertial>
  </link>
  <gazebo reference="back_yaw_link">
    <material>Gazebo/Blue</material>
  </gazebo>
  <joint name="back_yaw_joint" type="continuous">
    <origin rpy="0 0 0" xyz="-0.15 0 -0.073"/>
    <parent link="base_plate_link"/>
    <child link="back_yaw_link"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" velocity="100.0"/>
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <joint name="back_roll_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="back_yaw_link"/>
    <child link="back_roll_link"/>
    <axis xyz="1 0 0"/>
    <limit effort="1000.0" velocity="100.0"/>
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <joint name="back_pitch_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="back_roll_link"/>
    <child link="back_pitch_link"/>
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" velocity="100.0"/>
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  
  <gazebo>
    <plugin filename="libgazebo_ros_joint_state_publisher.so" name="barista_bot_joint_state_publisher">
      <ros>
        <remapping>~/out:=joint_states</remapping>
      </ros>
      <update_rate>50</update_rate>
      <joint_name>left_wheel_joint</joint_name>
      <joint_name>right_wheel_joint</joint_name>
      <joint_name>front_roll_joint</joint_name>
      <joint_name>front_pitch_joint</joint_name>
      <joint_name>front_yaw_joint</joint_name>
      <joint_name>back_roll_joint</joint_name>
      <joint_name>back_pitch_joint</joint_name>
      <joint_name>back_yaw_joint</joint_name>
    </plugin>
  </gazebo>
</robot>
